<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Budget Maker{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
            <link href="{{ asset('build/css/app.css') }}" rel="stylesheet" type="text/css">
        {% endblock %}

        {% block javascripts %}
            <script src="{{ asset('build/js/app.js') }}" type="text/javascript"></script>
        {% endblock %}
    </head>
    <body>
        {% block navbar %} {{ include('base/common-elements/navbar.html.twig') }} {% endblock %}
        {% block content %}
            {% block header %} {{ include('base/common-elements/header.html.twig') }} {% endblock %}
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-md-12">
                    {% block body %}{% endblock %}
                    </div>
                </div>
            </div>
        {% endblock %}
    </body>
</html>
